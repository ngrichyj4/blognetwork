<%if flash[:error]%>
<div align="center" class="alert alert-error" style="position:relative; width:465px; left:400px; top:37px; ">
        <button type="button" class="close" data-dismiss="alert">×</button>
        <strong>Oh snap!</strong> <%= flash[:error] %>
</div>
<%end%>

<%if flash[:success]%>
<div align="center" class="alert alert-success" style="position:relative; width:465px; left:400px; top:37px; ">
        <button type="button" class="close" data-dismiss="alert">×</button>
        <strong>Yuppers!</strong> <%= flash[:success] %>
</div>
<%end%>


<%if flash[:warning]%>
<div align="center" class="alert alert-block" style="position:relative; width:465px; left:500px; top:37px; ">
  <button type="button" class="close" data-dismiss="alert">×</button>
  <h4>Heads up!</h4>
  <%= flash[:warning] %>
</div>
<%end%>

<div id="main-content">   <!-- /main-content -->

	<%= yield %>
</div> <!-- /main-content -->
    

